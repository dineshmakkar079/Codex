{
  "manifest_version": 2,

  "name": "Codex",
  "description": "Launches VS IDE with current page problem statement",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
      {
        "matches": [ "https://*/*","http://*/*"],
        "js": ["jquery-3.2.1.min.js","myscript.js"]
      }
  ],
  "permissions": [
    "activeTab"
  ]
}
